{
  "docking": {
    "header": {
      "logging": {
        "logfile": "<your directory>/docking.log"
      }
    },
    "ligand_preparation": {
      "embedding_pools": [
        {
          "pool_id": "RDkit",
          "type": "RDkit",
          "parameters": {
            "protonate": true,
            "coordinate_generation": {
              "method": "UFF",
              "maximum_iterations": 600
            }
          },
          "input": {
            "standardize_smiles": false,
            "type": "console"
          }
        }
      ]
    },
    "docking_runs": [
      {
        "backend": "AutoDockVina",
        "run_id": "AutoDockVina",
        "input_pools": ["RDkit"],
        "parameters": {
          "binary_location": "<AutoDock Vina binary location>/autodock_vina_1_1_2_linux_x86/bin",
          "parallelization": {
            "number_cores": 36
          },
          "seed": 42,
          "receptor_pdbqt_path": [
            "<DRD2 AutoDock Vina Grid location>/grid_with_waters.pdbqt"
	  ],
          "number_poses": 1,
          "search_space": {
            "--center_x": 9.93,
            "--center_y": 5.85,
            "--center_z": -9.58,
            "--size_x": 15,
            "--size_y": 15,
            "--size_z": 15
          }
        },
        "output": {
          "poses": {
            "poses_path": "<where to save the docked poses>/docked_poses/poses.sdf"},
	  "scores": {
            "scores_path": "<where to save the docked scores>/docked_scores/scores.csv"	  }
      }
      }
    ]
  }
}
